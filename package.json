{
  "name": "nuxtjs-pwa-example",
  "version": "0.0.1",
  "description": "Nuxt environment with PWA support",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "micro": "micro ./services",
    "test": "ava",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "favicon": "real-favicon generate faviconDescription.json faviconData.json outputDir",
    "ava": {
      "require": [
        "babel-register"
      ]
    },
    "babel": {
      "presets": [
        "es2015"
      ]
    }
  },
  "author": "Marcus Baer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.26.2",
    "micro": "^7.0.6",
    "node-sass": "^4.5.0",
    "nuxt": "latest",
    "path": "^0.12.7",
    "sass-loader": "^6.0.2"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "5000"
    }
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "cli-real-favicon": "^0.0.6",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.0",
    "jsdom": "^9.11.0"
  }
}
